package gameLogic {		import flash.events.Event;		/* This class encapsulates the waiting area of the airport */public class WaitingArea {	//List of passangers in the waiting area	private var passengers:Array //Passengers		//Adds a passenger to the waiting area	public function pushPassenger(pass:Passenger):void {		passengers.push(pass);		pass.addEventListener(Passenger.OUTOFTIME, removePassenger);	    pass.addEventListener(Passenger.OUTOFMOOD, removePassenger);	}		private function removePassenger(e:Event):void {		trace(passengers.length);		passengers = passengers.filter(findPass);		trace(passengers.length);				function findPass(item:*, index:int, array:Array):Boolean {			return (!(e.target == item))		}	}		//PRE: Passenger must be in waiting area	//POST: Removes the passenger from the waiting area	public function popPassenger(pass:Passenger):void {		passengers = passengers.filter(findPass);						function findPass(item:*, index:int, array:Array):Boolean {			if (pass == item) {				item.removeEventListener(Passenger.OUTOFTIME, removePassenger);				item.removeEventListener(Passenger.OUTOFMOOD, removePassenger);			}			return (!(pass == item))		}	}	}}